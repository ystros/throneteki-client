{
  "presets": [
    "@babel/react",
    "@babel/env"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-assign",
    "@babel/plugin-proposal-object-rest-spread",
    ["react-css-modules", {
      "generateScopedName": "[name]__[local]___[hash:base64:5]",
      "filetypes": {
        ".scss": {
          "syntax": "postcss-less"
        }
      },
      "webpackHotModuleReloading": true
    }]
  ]
}
